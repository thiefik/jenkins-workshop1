pipeline {
    agent any
    tools {
        jdk 'jdk8'
        maven "mvn363"
    }
    stages {
        stage('GIT') {
            steps {

                        
                scm: [
$class: 'GitSCM', branches: [
[name: "${BRANCH_NAME}"]], doGenerateSubmoduleConfigurations: false, gitTool: 'Default', submoduleCfg: [], userRemoteConfigs: [[url: "https://github.com/jenkinsci/jenkins.git"]
]
]
                        
            }
        }
       
        stage("MVN"){
           steps{
                sh "mvn clean verify"      

            }
        }
    }
}

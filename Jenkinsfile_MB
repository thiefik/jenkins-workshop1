pipeline {
    agent any
    tools {
        jdk 'jdk8'
        maven "mvn363"
    }
    stages {
        stage('GIT') {
            steps {
                git url: 'https://github.com/thiefik/jenkins-workshop1.git', branch: '$BRANCH_NAME'                                    }
        }
       
        stage("MVN"){
           steps{
                sh "mvn clean verify"      

            }
        }
    }
}

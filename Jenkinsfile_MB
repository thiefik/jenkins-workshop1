pipeline {
    agent any
    tools {
        jdk 'jdk8'
        maven "mvn363"
    }
    stages {
        stage('GIT') {
            steps {
                git branch: "{$env.BRANCH_NAME}",
                        url: 'https://github.com/jenkinsci/jenkins.git'
            }
        }
       
        stage("MVN"){
           steps{
                sh "mvn clean verify"      

            }
        }
    }
}
